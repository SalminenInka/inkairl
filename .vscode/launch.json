{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "fetch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/dist/fetcher.js",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    }
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "JSfetch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/jsfetcher.js",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    }
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "write",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/writer.js",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    }
  ]
}